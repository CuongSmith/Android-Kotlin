<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/la1"
        android:layout_width="205dp"
        android:layout_height="150dp"
        android:background="@android:color/holo_red_light"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/la2"
        android:layout_width="205dp"
        android:layout_height="150dp"
        android:background="@android:color/holo_purple"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@id/la1"
        tools:ignore="MissingConstraints" />
    
    <ImageView
        android:id="@+id/la3"
        android:layout_width="82dp"
        android:layout_height="150dp"
        android:background="@android:color/darker_gray"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/la1"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/la4"
        android:layout_width="82dp"
        android:layout_height="150dp"
        android:background="@android:color/holo_blue_dark"
        app:layout_constraintLeft_toRightOf="@id/la3"
        app:layout_constraintTop_toBottomOf="@id/la1"/>

    <ImageView
        android:id="@+id/la5"
        android:layout_width="82dp"
        android:layout_height="150dp"
        android:background="@android:color/holo_blue_bright"
        app:layout_constraintLeft_toRightOf="@id/la4"
        app:layout_constraintTop_toBottomOf="@id/la1"/>

    <ImageView
        android:id="@+id/la6"
        android:layout_width="82dp"
        android:layout_height="150dp"
        android:background="@android:color/holo_green_dark"
        app:layout_constraintLeft_toRightOf="@id/la5"
        app:layout_constraintTop_toBottomOf="@id/la2"/>

    <ImageView
        android:id="@+id/la7"
        android:layout_width="82dp"
        android:layout_height="150dp"
        android:background="@android:color/white"
        app:layout_constraintTop_toBottomOf="@id/la2"
        app:layout_constraintLeft_toRightOf="@id/la6"
        app:layout_constraintRight_toRightOf="parent"/>

    <ImageView
        android:id="@+id/la8"
        android:layout_width="205dp"
        android:layout_height="130dp"
        android:background="@android:color/holo_orange_dark"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/la4"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/la9"
        android:layout_width="205dp"
        android:layout_height="130dp"
        android:background="@android:color/holo_green_light"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/la6"
        app:layout_constraintLeft_toRightOf="@id/la8"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/la10"
        android:layout_width="match_parent"
        android:layout_height="140dp"
        android:background="@android:color/black"
        app:layout_constraintTop_toBottomOf="@id/la9"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/la11"
        android:layout_width="match_parent"
        android:layout_height="161dp"
        app:layout_constraintTop_toBottomOf="@id/la10"
        android:background="@android:color/holo_purple"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="MissingConstraints" />




</androidx.constraintlayout.widget.ConstraintLayout>